{
  "name": "stelianandrei/laravel-server-side-ga",
  "description": "A simple Laravel package that allows the sending of Google Analytics events from the server side",
  "keywords": [
    "laravel",
    "server-side",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/StelianAndrei/laravel-server-side-ga",
  "require": {
    "php": "^7.0.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "StelianAndrei\\LaravelServerSideGA\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "StelianAndrei\\LaravelServerSideGA\\AnalyticsServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Stelian-Andrei Adumitroaei",
      "email": "stelian.andrei@gmail.com"
    }
  ]
}
